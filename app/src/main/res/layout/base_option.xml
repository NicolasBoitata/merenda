        android:paddingTop="150dp"
        android:paddingLeft="22dp">

        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/menuToolbar1"
            android:layout_width="172dp"
            android:layout_height="235dp"
            android:background="@drawable/menu_toolbar"
            android:minHeight="?attr/actionBarSize"
            android:theme="?attr/actionBarTheme"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            tools:ignore="DuplicateIds" />

        <ImageView
            android:id="@+id/imageView1"
            android:layout_width="138dp"
            android:layout_height="100dp"
            android:layout_alignTop="@+id/menuToolbar1"
            android:layout_alignEnd="@+id/menuToolbar1"
            android:layout_alignParentStart="true"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="20dp"
            android:paddingTop="10dp"
            android:src="@drawable/chocolate"
            app:layout_constraintEnd_toEndOf="@+id/menuToolbar1"
            app:layout_constraintStart_toStartOf="@+id/menuToolbar1"
            app:layout_constraintTop_toTopOf="@+id/menuToolbar1"
            tools:layout_editor_absoluteX="227dp"
            tools:layout_editor_absoluteY="468dp" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/imageView1"
            android:layout_alignParentStart="true"
            android:layout_marginStart="25dp"
            android:layout_marginTop="5dp"
            android:text="@string/view_titulo"
            android:textColor="@color/black"
            android:textSize="15dp"
            tools:layout_editor_absoluteX="236dp"
            tools:layout_editor_absoluteY="568dp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
